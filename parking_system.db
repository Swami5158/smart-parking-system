CREATE TABLE vehicles (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    number_plate TEXT NOT NULL,
    vehicle_type TEXT,
    slot_number INTEGER,
    entry_time TEXT
);

CREATE TABLE history (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    number_plate TEXT NOT NULL,
    vehicle_type TEXT,
    slot_number INTEGER,
    entry_time TEXT,
    exit_time TEXT,
    payment_amount REAL
);
